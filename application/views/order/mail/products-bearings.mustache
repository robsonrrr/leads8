<tr style="height:1px; margin:0; padding:0; border:0;">
    <td>&nbsp;</td>
    <td width="100%"colspan="2"></td>
    <td>&nbsp;</td>
</tr>

<!------------------------------------------------------------------------------------------
Check List e Total Lead
------------------------------------------------------------------------------------------->
<tr style="height:150px">
    <td>&nbsp;</td>
    <td valign="top">
        <table width="100%" border="0" cellspacing="0" cellpadding="3"  bgcolor="#FFFFFF" align="left">
            {{#Prazo}}
            <tr style=" height:20px; line-height:20px; font-size:13px;color:#000; vertical-align:middle ">
                <td align="left">Prazo: <strong> {{PrazoTermos}}</strong></td>
            </tr> 
            {{/Prazo}}
            {{#TipoPagamento}}
            <tr style=" height:20px; line-height:20px; font-size:13px;color:#000; vertical-align:middle ">
                <td align="left">Pagamento: <strong>{{pagamentoDescricao}}</strong></td>
            </tr>
            {{/TipoPagamento}}
            {{#Natureza}}
            <tr style=" height:20px; line-height:20px; font-size:13px;color:#000; vertical-align:middle ">
                <td align="left">Natureza: <strong>{{naturezaOperacao}}</strong></td>
            </tr>
            {{/Natureza}}
            {{#pedidoDataEntregaProgramadaFormatado}}
            <tr style=" height:20px; line-height:20px; font-size:13px;color:#000; vertical-align:middle ">
                <td align="left">
                    Data Entrega: <strong>{{pedidoDataEntregaProgramadaFormatado}}</strong>  
                </td>
            </tr>
            {{/pedidoDataEntregaProgramadaFormatado}}
            {{#Transportadora}}
            <tr style=" height:20px; line-height:20px; font-size:13px;color:#000; vertical-align:middle ">
                <td align="left">
                    Coleta: <strong>{{transportadoraNome}}</strong>  
                </td>
            </tr>
            {{/Transportadora}}
            <tr style=" height:20px; line-height:20px; font-size:13px;color:#000; vertical-align:middle ">
                <td align="left">Peso: <strong>{{Total.peso}} Kg</strong></td>
            </tr>
        </table>
    </td>
    {{#Total}}
    <td valign="top">
        <table width="75%" border="0" cellspacing="0" cellpadding="5"  bgcolor="#FFFFFF" align="right" style=" border:0;" >
            <tr style=" height:15px; line-height:15px; font-size:12px;color:#000; vertical-align:top ">
                <td align="right" style="" >Total em Produtos</td>
                <td align="right" style=" border-left:2px solid #f8f8f8" ><strong>R$ {{produto}}</strong></td>
            </tr>
            <tr style=" height:15px; line-height:15px; font-size:12px;color:#000; vertical-align:top ">
                <td align="right"   style="" >Total de IPI</td>
                <td align="right"   style=" border-left:2px solid #f8f8f8" ><strong>R$ {{ipi}}</strong></td>
            </tr>
            <tr style=" height:15px; line-height:15px; font-size:12px;color:#000; vertical-align:top ">
                <td align="right"   style="" >Total de ST</td>
                <td align="right"   style=" border-left:2px solid #f8f8f8" ><strong>R$ {{st}}</strong></td>
            </tr>
            {{#frete}}
            <tr style=" height:15px; line-height:15px; font-size:12px;color:#000; vertical-align:top ">
                <td align="right" style="" >Valor do Frete</td>
                <td align="right" style=" border-left:2px solid #f8f8f8" ><strong>R$ {{frete}}</strong></td>
            </tr>
            {{/frete}}
            <tr style=" height:15px; line-height:15px; font-size:14px;color:#000; vertical-align:top; background-color:{{Mail.color}}; color:#000; font-weight:bolder ">
                <td align="right" >Total do Pedido </td>
                <td align="right" ><strong>R$ {{pedido}}</strong></td>
            </tr>
        </table>
    </td>
    {{/Total}}
    <td>&nbsp;</td>
</tr>